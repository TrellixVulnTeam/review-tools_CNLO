#!/usr/bin/python3

from clickreviews import frameworks
import sys

def main():
    filename = None
    if len(sys.argv) == 2:
        filename = sys.argv[1]
    frameworks.get_frameworks_file(filename)

if __name__ == '__main__':
    try:
        main()
    except KeyboardInterrupt:
        print('Aborted.', file=sys.stderr)
        sys.exit(1)
